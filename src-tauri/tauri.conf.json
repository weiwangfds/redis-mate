{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RedisMate",
  "version": "0.1.0",
  "identifier": "com.wei.redis-mate",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "RedisMate",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "untrusted comment: minisign public key: 28576FEEE9B145DD\nRWTdRbHp7m9XKNOgJqidHUJZs69C1CGU5r++JTKWt1EtVCZsFQmHGgKC",
      "endpoints": [
        "https://github.com/weiwangfds/redis-mate/releases/latest/download/latest.json"
      ]
    }
  }
}
